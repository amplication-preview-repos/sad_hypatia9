datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Location {
  createdAt   DateTime @default(now())
  description String?
  id          String   @id @default(cuid())
  latitude    Float?
  longitude   Float?
  name        String?
  updatedAt   DateTime @updatedAt
}
